#include <bits/stdc++.h>
using namespace std;
 
char board[8][8];
int n = 8;
int cnt = 0;
vector<bool> column(15,0);
vector<bool> diag1(15,0);
vector<bool> diag2(15,0);
 
void search(int y){
    if(y==n){
        cnt++;
        return;//must add
    }
    for(int x=0;x<n;++x){
        if(column[x]||diag1[x+y]||diag2[x-y+n-1]||board[y][x]=='*') continue;
 
        column[x]=diag1[x+y]=diag2[x-y+n-1]=1;
        search(y+1);
        column[x]=diag1[x+y]=diag2[x-y+n-1]=0;
 
    }
 
}
 
int main() {
	for(int i=0;i<8;++i){
        for(int j=0;j<8;++j){
            cin>>board[i][j];
        }
    }
 
    search(0);
    cout<<cnt;
 
 
    return 0;
}
